emoji = "‚≠ê"
H = [
    emoji + "   " + emoji,
    emoji + "   " + emoji,
          emoji*3,
    emoji + "   " + emoji,
    emoji + "   " + emoji
]
I = [
    " " + emoji*3 + " ",
    "   " + emoji + "  ",
    "    " + emoji + "  ",
    "   " + emoji + "  ",
    " "+emoji*3+" "
]
for h, i in zip(H, I):
    print(h + "   " + i)
